{
    "name": "Starknet Dojo Development",
    "image": "bravardev/starknet-dojo-dev:main",
    "customizations": {
        "vscode": {
            "extensions": [
                "StarkWare.cairo1",
                "tamasfe.even-better-toml",
                "redhat.vscode-yaml"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash",
                "terminal.integrated.profiles.linux": {
                    "bash": {
                        "path": "bash"
                    }
                }
            }
        }
    },
    "runArgs": [],
    "forwardPorts": [3000, 5173, 5174, 8080, 5050, 5051, 5052],
    "postCreateCommand": "dojoup",
    "remoteUser": "dojo"
}
